{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "homepage": "/static/",
  "scripts": {
    "build": "vite build",
    "dev": "chokidar \"src/**/*\" \"public/**/*\" -c \"npm run sync-build\"",
    "sync-build": "vite build && rm -rf ../static/* ../backend/templates/* && cp -r dist/assets ../static/ && cp dist/index.html ../inzynierka/templates/"
  },
  "dependencies": {
    "axios": "^1.12.2",
    "chokidar-cli": "^3.0.0",
    "jwt-decode": "^4.0.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.9.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^5.0.0",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "vite": "^7.1.2"
  }
}
